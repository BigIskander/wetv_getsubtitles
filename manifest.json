{
    "manifest_version": 3,
    "name": "WeTV get subtitles",
    "description": "A simple browser extension to open subtitles page on wetv.vip",
    "version": "1.0",
    "action": {
      "default_icon": "WeTV.png"
    },
    "permissions": ["activeTab", "scripting", "webRequest"],
    "host_permissions": [
      "https://wetv.vip/*",
      "https://cffaws.wetvinfo.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
  }